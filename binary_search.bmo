binary_search(array, num):
	0 -> lo
	2 -> hi
	while lo < hi:
		lo + (hi - lo) / 2 -> mid
		if array[mid] == num:
			return mid
		if array[mid] > num:
			mid -> hi
		if array[mid] < num:
			mid + 1 -> lo
	-1

main:
	{1, 2, 3, 4, 5, 6, 7, 8, 9} -> array
	{2, 3, 4} -> ar
	print "The index is: "
	print binary_search(ar, 3)
